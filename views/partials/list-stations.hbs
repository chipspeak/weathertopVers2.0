{{#each stations}}
<div class="box box-link-hover-shadow">
  <div class="title">
    {{location}}
  </div>
  {{>list-latest-reading}}
  <a href="/station/{{_id}}" class="button is-info">
    <i class="fa-solid fa-folder-open"></i>
  </a>
  <a href="/dashboard/deletestation/{{_id}}" class="button is-danger">
    <i class="fa-solid fa-trash"></i>
  </a>
</div>
{{/each}}